#include <iostream>
#include<stdlib.h>
using namespace std;

class Stack
{
    class Element
    {
        public:
            Element* prev;
            float value;
    };
public:
    Element* t;


    Stack();    // inicjalizuje pusty stos
    ~Stack();   // kasuje wszystkie elementy

    void pop();   // kasuje gorny element stosu
    float top();  // zwraca wartosc na gorze stosu

    bool empty();     // true = stos jest pusty
    void push(float); // umieszcza wartosc na wierzcholku stosu
};

Stack::Stack(){
    t = new Element;
}

Stack::~Stack(){
    delete this->t;
}

void Stack::pop(){
    t=t->prev;
}

float Stack::top(){
    return t->value;
}

bool Stack::empty(){
    if (this->top()==0) return true;
    else return false;
}

void Stack::push(float x){
    Element* A;
    A=new Element;
    A=t;

    t=new Element;
    t->value=x;
    t->prev=A;

    delete A;
}

