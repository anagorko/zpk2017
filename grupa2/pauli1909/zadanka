//zadanie1
#include <iostream>
using namespace std;

int main() {
	int a=1,b=2,c=3;
cin >>a;
cin>>b;
cin>>c;
cout<<a<<" "<<b<<" "<<c<<" "<<endl;
cout<<c<<" "<<b<<" "<<a<<" "<<endl;
}

//zadanie2
#include <iostream>
using namespace std;

int main() {
	int  a=4000;
	cout<<(1*(int)((a/3600)/1.+0.1))<<"g"<<(a-(1*(int)((a/3600)/1.+0.1))*3600)/60<<"m"<<a-((1*(int)((a/3600)/1.+0.1))*3600)-((a-(1*(int)((a/3600)/1.+0.1))*3600)/60)*60 <<"s"<<endl;
}
