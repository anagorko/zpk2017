	}else
	cout<<b<<endl;
}

//zadanie5
#include <iostream>
using namespace std;

int main() {
	int a=1, b=10, c=2;
	int h=((b^2)+((1/2*c))^2)^(1/2);
	if (a+b>c && a+c>b && b+c>b){
		cout<<"TAK"<<endl;
	}else
		cout<<"NIE"<<endl;
}
//zadanie6
#include <iostream>
using namespace std;

int main() {
	int x=0, y=0;
	if(x>0 && y>0){
	cout<<"I"<<endl;
	}else if (x>0 && y<0){
		cout<<"II"<<endl;
	}else if (x<0 && y<0){
		cout<<"III"<<endl;
	}else if(x<0 && y>0){
		cout<<"IV"<< endl;
	}else if (x==0 && y!=0){
		cout<<"0X"<<endl;
	}else if (x!=0 && y==0){
	cout<<"0Y"<<endl;
	}
	else
	cout<<"00"<<endl;
}

//zadanie7
#include <iostream>
using namespace std;

int main() {
	int a=12, b=8, c=4;
	if(a>b){
		cout<<"ilosc krzesel: "<<a/c*2 + (b-2*c)/c*2<<endl;
	}
	else
	cout<<"ilosc krzesel: "<<b/c*2 + (a-2*c)/c*2<<endl;
	
}
